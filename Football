import java.util.Scanner;
import java.io.*;
 
public class Main{
public static void main(String args[])
{
   
    Scanner scan=new Scanner(System.in);
    
    String s;
    String g="1111111";
    String f="0000000";
     s=scan.nextLine();
    if(s.indexOf(g)>=0||s.indexOf(f)>=0)
    {
        System.out.println("YES");
    }
    else
    {
        System.out.println("NO");
    }
   
    
}
 
}
