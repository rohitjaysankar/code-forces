import java.io.*;
import java.util.*;
public class Main{
    public static void main(String args[])
    {
        Scanner scan=new Scanner(System.in);
        int g=scan.nextInt();
        if(g<=5)
        {
            System.out.println("1");
        }
        else
        {
            if(g%5==0)
            {System.out.println(g/5);}
            else
            {System.out.println((g/5)+1);}
        }
    }
}
