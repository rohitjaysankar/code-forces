import java.util.*;
import java.io.*;
import java.lang.*;
 
public class Main
{
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int k,b;
		k=0;b=0;
		int arr[][]=new int [5][5];
		for(int i=0;i<5;i++)
		{
		    for(int j=0;j<5;j++)
		    {
		        arr[i][j]=scan.nextInt();
		        if(arr[i][j]==1)
		        {
		            k=i;
		            b=j;
		        }
		    }
		}
		int d=Math.abs(k-2)+Math.abs(b-2);
		System.out.println(d);
	}
}
